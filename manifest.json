  "prefer_related_applications": false,
  
  "protocol_handlers": [
    {
      "protocol": "web+dutchportal",
      "url": "/?action=%s"
    },
    {
      "protocol": "web+techno",
      "url": "/events?genre=%s"
    }
  ],
  
  "file_handlers": [
    {
      "action": "/music/upload",
      "accept": {
        "audio/*": [".mp3", ".wav", ".flac", ".m4a"]
      }
    }
  ],
  
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "music",
          "accept": ["audio/*", "video/*"]
        },
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  
  "handle_links": "preferred",
  
  "display_override": ["window-controls-overlay", "minimal-ui", "standalone", "browser"],
  
  "capture_links": "existing-client-navigate",
  
  "id": "dutch-underground-portal",
  
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  
